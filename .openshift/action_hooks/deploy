
sed -i 's/OPENSHIFT_MYSQL_DB_USERNAME/'$OPENSHIFT_MYSQL_DB_USERNAME'/' $OPENSHIFT_REPO_DIR/config.php
sed -i 's/OPENSHIFT_MYSQL_DB_HOST/'$OPENSHIFT_MYSQL_DB_HOST'/' $OPENSHIFT_REPO_DIR/config.php
sed -i 's/OPENSHIFT_MYSQL_DB_PORT/'$OPENSHIFT_MYSQL_DB_PORT'/' $OPENSHIFT_REPO_DIR/config.php
sed -i 's/OPENSHIFT_MYSQL_DB_PASSWORD/'$OPENSHIFT_MYSQL_DB_PASSWORD'/' $OPENSHIFT_REPO_DIR/config.php
sed -i 's/OPENSHIFT_APP_NAME/'$OPENSHIFT_APP_NAME'/' $OPENSHIFT_REPO_DIR/config.php

